[
    {
        "identifier": "lf_users_list",
        "request": "GET",
        "path": "members",
        "route": "MemberRoutes",
        "action": "getall"
    },
    {
        "identifier": "lf_users_list_filter",
        "request": "GET",
        "path": "members/filter/:filter",
        "route": "MemberRoutes",
        "action": "getfilter",
        "gets": [
            {
                "name": ":filter",
                "regex": "w"     
            }
        ]
    },
    {
        "identifier": "lf_users_getone",
        "request": "GET",
        "path": "members/:id",
        "route": "MemberRoutes",
        "action": "getone",
        "gets": [
            {
                "name": ":id",
                "regex": "d"
            }
        ]
    },
    {
        "identifier": "ldf_users_addone",
        "request": "POST",
        "path": "members",
        "route": "MemberRoutes",
        "action": "add"
    },
    {
        "identifier": "ldf_users_putone",
        "request": "PUT",
        "path": "members/:id",
        "route": "MemberRoutes",
        "action": "edit",
        "gets": [
            {
                "name": ":id",
                "regex": "d" 
            }
        ]
    }
]
          